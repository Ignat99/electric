<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE ErrorLogger
 [
 <!ELEMENT ERRORTYPELINE ANY>
 <!ATTLIST ERRORTYPELINE
    p1 CDATA #REQUIRED
    p2 CDATA #REQUIRED
    cellName CDATA #REQUIRED
 >
 <!ELEMENT ERRORTYPETHICKLINE ANY>
 <!ATTLIST ERRORTYPETHICKLINE
    p1 CDATA #REQUIRED
    p2 CDATA #REQUIRED
    cellName CDATA #REQUIRED
 >
 <!ELEMENT ERRORTYPEPOINT ANY>
 <!ATTLIST ERRORTYPEPOINT
    pt CDATA #REQUIRED
    cellName CDATA #REQUIRED
 >
 <!ELEMENT ERRORTYPEPOLY (ERRORTYPETHICKLINE|ERRORTYPELINE)*>
 <!ATTLIST ERRORTYPEPOLY
    cellName CDATA #REQUIRED
    origCellName CDATA #IMPLIED
    origGeoName CDATA #IMPLIED
 >
 <!ELEMENT ERRORTYPEGEOM ANY>
 <!ATTLIST ERRORTYPEGEOM
    geomName CDATA #REQUIRED
    cellName CDATA #REQUIRED
 >
 <!ELEMENT ErrorLogger (GroupLog|MessageLog|WarningLog)*>
 <!ELEMENT GroupLog (MessageLog|WarningLog)*>
 <!ATTLIST ErrorLogger
    errorSystem CDATA #REQUIRED
 >
 <!ATTLIST GroupLog
    message CDATA #REQUIRED
 >
 <!ELEMENT MessageLog (ERRORTYPEGEOM|ERRORTYPETHICKLINE|ERRORTYPELINE|ERRORTYPEPOINT|ERRORTYPEPOLY)*>
 <!ATTLIST MessageLog
    message CDATA #REQUIRED
    cellName CDATA #REQUIRED
 >
 <!ELEMENT WarningLog ANY>
 <!ATTLIST WarningLog
    message CDATA #REQUIRED
    cellName CDATA #IMPLIED
 >
 ]>